<style>
  * {
    -webkit-font-smoothing: antialiased;
  }

  ::selection {
    color: #fff;
    background: #00DEAE;
  }

  :focus {
    outline: 0;
  }

  .w-nav-overlay {
    top: 0;
  }

  .form,
  .request-access-form {
    display: block !important;
  }

  .w-tab-link {
    background-color: transparent;
  }

  .w-tab-link.w--current {
    background-color: transparent;
  }

  .w-slider-dot {
    width: 8px;
    height: 8px;
    border-radius: .4rem;
    background-color: #DFDAE2 !important;
  }

  .w-slider-dot.w-active {
    width: 10px;
    height: 10px;
    background-color: #9A2F8F !important;
  }

.w-select {
    -webkit-appearance: none;
    -moz-appearance: none;
}
  
  
  textarea,
  input:matches([type="email"],
    [type="number"],
    [type="password"],
    [type="search"],
    [type="tel"],
    [type="text"],
    [type="url"]) {
    -webkit-appearance: none;
    -moz-appearance: none;
    appearance: none;
  }

  /* Material Icons */
  .material-icons,
  .material-icons-outlined,
  .material-icons-two-tone,
  .material-icons-round,
  .material-icons-sharp {
    font-weight: normal;
    font-style: normal;
    font-size: 20px;
    line-height: 24px;
    text-align: center;
    display: inline-block;
    text-transform: none;
    letter-spacing: normal;
    word-wrap: normal;
    white-space: nowrap;
    direction: ltr;
    -webkit-font-smoothing: antialiased;
    text-rendering: optimizeLegibility;
    -moz-osx-font-smoothing: grayscale;
    font-feature-settings: 'liga';
  }

  .material-icons-two-tone {
    color: red !important;
  }

  .material-icons-s {
    font-size: 16px;
    line-height: 16px;
  }

  /* Use case */
  .use-case-header .circle-icon {
    transition: all .25s;
  }

  .use-case-header:hover .circle-icon {
    background-color: #9A2F8F !important;
  }

  .use-case-header:hover .material-icons {
    color: #FFF !important;
  }
  .use-case-body-text p {
	font-size: 16px;
	line-height: 24px;
  }

  /* Intl-tel-input */
  .intl-tel-input {
    display: block;
  }

  .iti--allow-dropdown .iti__flag-container:hover .iti__selected-flag {
    border-radius: .4rem 0 0 .4rem !important;
  }

  .iti__country-list {
    border-radius: .4rem !important;
    border: 1px solid #DFDAE2 !important;
  }

  .iti__divider {
    border-bottom: 1px solid #DFDAE2 !important;
  }
</style>

