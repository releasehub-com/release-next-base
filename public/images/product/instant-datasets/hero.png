<script rel="preconnect">
  function getQueryVariable(variable) {
  var query = window.location.search.substring(1);
  var vars = query.split("&");
  for (var i = 0; i < vars.length; i++) {
    var pair = vars[i].split("=");
    if (decodeURIComponent(pair[0]) == variable) {
      return decodeURIComponent(pair[1]);
    }
  }
  return "";
}

function createSecretInput(name, value, node) {
  const el = document.createElement("input");
  el.setAttribute("type", "text");
  el.setAttribute("name", name);
  el.setAttribute("data-name", name);
  el.setAttribute("value", value);
  el.style.display = "none";
  node.insertBefore(el, node.firstChild);
}

function getCookie(name) {
  const value = `; ${document.cookie}`;
  const parts = value.split(`; ${name}=`);
  if (parts.length === 2)
    return parts
      .pop()
      .split(";")
      .shift();
  return '';
}

const hubspotutk = getCookie("hubspotutk");
$("[hubspot='form']").each(function() {
  ["utm_source", "utm_medium", "utm_content", "utm_campaign"].forEach(item =>
    createSecretInput(item, getQueryVariable(item), this)
  );

  createSecretInput("hubspotutk", hubspotutk, this);
});

